    "version": "0.2.0",
    "configurations": [
        {
            "name": "SegDet full",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false, 
            "args": [
                "--config-file-detic",
                "Detic/configs/Detic_LCOCOI21k_CLIP_SwinB_896b32_4x_ft4x_max-size.yaml",
                "--input",
                "inputs/uvas/Captura5.png",
                "--output",
                "output/",
                "--vocabulary",
                "custom",
                "--custom_vocabulary",
                "grape, botrytis_grape",
                "--confidence-threshold",
                "0.15",
                "--nms_max_overlap",
                "0.3",
                "--xdec_img_size", "512",
                "--vocabulary_xdec", "closer vineyard",
                "--debug",
                "--det_model", "YOLO"
            ]
        },
        {
            "name": "SegDet patch",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false, 
            "args": [
                "--full_pipeline",
                "--config-file-detic",
                "Detic/configs/Detic_LI_CLIP_SwinB_896b32_4x_ft4x_max-size.yaml",
                "--input",
                "inputs/Pilot1/row_8_1/default/images/*.png",
                "--output",
                "output/",
                "--vocabulary",
                "custom",
                "--custom_vocabulary",
                "grape",
                "--confidence-threshold",
                "0.15",
                "--nms_max_overlap",
                "0.5",
                "--patching",
                "--patch_size",
                "640",
                "--overlap",
                "0.2",
                "--xdec_img_size", "512",
                "--vocabulary_xdec", "closer vineyard",
                "--debug",
                "--det_model", "YOLO"
            ]
        }
    ]
